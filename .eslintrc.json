{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
        "import/no-extraneous-dependencies": "off"
    },
    "plugins": [
        "jest"
    ]
}